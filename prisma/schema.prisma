
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Attendant  {
id Int @id @default(autoincrement())
name String
last_name String
email String @unique
password String 
services Services[]
}

model Services {
  id Int @id @default(autoincrement())
  type String
  attendant Attendant @relation(fields: [id], references: [id])
}
