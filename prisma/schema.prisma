
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Attendant  {
id Int @id @default(autoincrement())
name String
last_name String
email String @unique
password String 
adm Boolean
services Services[]
}

model Services {
  id Int @id @default(autoincrement())
  type String
  register String? 
  created_at DateTime
  id_attendant Int
  attendant Attendant @relation(fields: [id_attendant], references: [id])
  id_rating    Int?  @unique
  rating      Rating?   @relation(fields: [id_rating], references: [id])
}

model Rating {
  id Int @id @default(autoincrement())
  questions String
  id_service Int?
  service Services?
}